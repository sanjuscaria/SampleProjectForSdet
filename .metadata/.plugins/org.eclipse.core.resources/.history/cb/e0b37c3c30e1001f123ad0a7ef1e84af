package codeTesting;

public class MethodHiding {
	
	public void testing()
	{
		
	}
	
	public static void main(String[] args)
	{
		
		Parent p = new child();
		
	}

}

class Parent {
	
	void age()
	{
		System.out.println("parent's age");
	}
}

class Child extends Parent {
	
	void age()
	{
		System.out.println("child's age");
	}
}
