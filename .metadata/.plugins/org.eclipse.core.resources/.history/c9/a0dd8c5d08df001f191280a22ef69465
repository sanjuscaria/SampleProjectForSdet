package codeTesting;

public class ArrayDuplicates {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int[] a = { 5, 2, 1, 5, 2, 6 };

		int count = 0;

		for (int i = 0; i < a.length; i++) {
			for (int j = 0; j < a.length; j++) {
				if (a[i] == a[j]) {
					count ++;
					
				}
				if(count > 1)
				{
					System.out.println("Element "+a[i]+" repeated "+count+" times");
				}

			}

		}

	}

}
