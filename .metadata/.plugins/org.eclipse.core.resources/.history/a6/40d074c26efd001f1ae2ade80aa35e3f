class Person{
	
	static void getSalary() {
		
		System.out.println("from person");
	
	}
	void nationality()
	{
		System.out.println("from person");
		
	}
	
}

class Indian extends Person{
	
	static void getSalary()
	{
		System.out.println("from indian");
	}
	@Override
	void nationality()
	{
		System.out.println("from indian");
	}
}