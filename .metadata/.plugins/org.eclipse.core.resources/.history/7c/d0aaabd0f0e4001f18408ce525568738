package codeTesting;

public class AnonymousClass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Employee emp = new Employee() {
			public void getAge()
			{
				System.out.println("from ananymous class...");
			}
		};
		
		emp.getAge();

	}

}

class Employee{
	
	public void getAge()
	{
		System.out.println("emplyoee class...");
	}
	
}
